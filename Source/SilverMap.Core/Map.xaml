<UserControl x:Class="Ptvag.Dawn.Controls.SilverMap.Core.Map" 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" d:DesignHeight="200" d:DesignWidth="300" >
    <Grid Background="Transparent">
        <Canvas Canvas.ZIndex="128" x:FieldModifier="public" x:Name="TopPaneCanvas" />
        <Grid x:FieldModifier="public" x:Name="Layers" >
            <Canvas Canvas.ZIndex="128" x:FieldModifier="public" x:Name="ForePaneCanvas"/>
            <Canvas Canvas.ZIndex="64" x:FieldModifier="public" x:Name="GeoCanvas"/>
            <Grid Canvas.ZIndex="32" x:FieldModifier="public" x:Name="BackPaneCanvas"/>
        </Grid>
    </Grid>
</UserControl>